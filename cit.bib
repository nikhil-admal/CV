@Unpublished{pitteri:zanzotto:1996,
  author       = {Mario Pitteri and Giovanni Zanzotto},
  title	       = {Symmetry breaking and transformation twinning},
  note	       = {Submitted to } # arch-r-mech,
  year	       = 1996,
  abstract     = {The investigation of microstructure is largely based on the
                  knowledge of the transformation twins the are possible for a
                  crystal undergoing a symmetry-breaking, diffusion-less phase
                  transition. In order to have a complete description of this
                  phenomenon, in this paper we review the subject of mechanical
                  twinning from the point of view of nonlinear elasticity, and
                  we examine the existence and genericity of all transformation
                  twins, that is, all the rank-1 connections between the
                  low-symmetry potential-wells occurring in a suitable
                  neighborhood of the high-symmetry well for all the
                  symmetry-breaking transitions that are possible in
                  three-dimensional simple lattices. The analysis also shows
                  that there are exceptions to the so-called ``Mallard law'',
                  according to which the twins arise generically from lost
                  symmetry planes and axes in the parent phase, and are
                  conjugate twins of Type 1 and 2, also called conventional
                  twins. In fact, we see that generic twins not of Type 1 or 2
                  do exist, and we describe all of them here, together with
                  various cases of non-conventional non-generic twins that can
                  exist in the transitions to monoclinic and triclinic
                  symmetries if the transition strains satisfy certain
                  relations that we give explicitly. It is remarkable that in
                  such non-generic transitions the greater number of twins
                  changes drastically the microstructures that the material can
                  form, and this may give away to control or enhance its
                  macroscopic properties. The analysis also shows that
                  Mallard's law holds if at least one of the twins in a
                  conjugate pair is required to have a material interface, or
                  if a restrictive notion of twin genericity is adopted.},
  hardcopy     = {yes},
  ecopy	       = {yes}
}

