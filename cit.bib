@Article{baskes:1987,
  author       = {M. I. Baskes},
  title	       = {Application of the embedded-atom method to covalent
                  materials: a semiempirical potential for silicon},
  journal      = phys-rev-l,
  year	       = 1987,
  volume       = 59,
  number       = 23,
  pages	       = {2666--2669},
  month	       = dec,
  abstract     = {The embedded-atom method, a semiempirical theory of metal
                  bonding, is investigated as a method to calculate the bonding
                  in a covalent material. A simple first-neighbor embedded-atom
                  method model is sufficient to explain the geometry and
                  structure of many metastable phases of silicon, but not its
                  shear behavior. To obtain realistic shear behavior of
                  silicon, an angle-dependent electron density is included to
                  model the effects of bond bending. This model uses only the
                  experimental bulk properties of silicon. Calculated
                  properties of metastable phases and point defects are
                  presented.},
  keywords     = {semiconductor embedded-atom-method covalent-materials
                  semiempirical-potential metal-bonding geometry structure
                  metastable-phases shear-behavior
                  angle-dependent-electron-density bond-bending point-defects
                  Si},
