@PhdThesis{shaw:1997,
  author       = {John A. Shaw},
  title	       = {Material Instabilities in a Nickel-Titanium Shape Memory
                  Alloy},
  school       = {The University of Texas at Austin},
  year	       = 1997,
  address      = {Austin, TX 78712},
  month	       = Jan,
  abstract     = {Uniaxial experiments of a nearly equiatomic NiTi alloy are
                  conducted in the temperature and deformation regime in which
                  it exhibits the \emph{shape memory effect} and
                  \emph{pseudoelasticity}. These remarkable characteristics
                  arise from the diffusionless transformation between two solid
                  state phases, \emph{austenite} and \emph{martensite}. Through
                  a systematic series of experiments, the loading rate and the
                  choice of ambient medium are found to have a significant
                  influence on the apparent mechanical response. Discrete local
                  measurements of strain and temperature show this to be caused
                  by a complex interaction between the temperature sensitivity
                  of the transformation stresses, self-heating or self-cooling
                  due to the latent heat of transformation, and the prevailing
                  heat transfer conditions of the experiment. A full field
                  monitoring scheme, using NiTi strip specimens, is developed
                  to track the nonuniform strain and temperature fields during
                  stress-induced phase transformations. Synchronized histories
                  of deformation, obtained by photographs of the disturbance of
                  a brittle coating on the surface, and temperature, obtained
                  by infrared thermal imaging, lead to a clear explanation of
                  the observed events. During loading (or unloading),
                  nucleation of the new phase is a distinct event requiring a
                  higher (lower) stress than that required to propagate the
                  transformation. This nucleation peak, coupled with the local
                  temperature changes, govern the number of nucleations. It is
                  also shown that under steady-state conditions coexisting
                  fronts all propagate at a speed that is inversely
                  proportional to the number of traveling fronts. Nucleation in
                  thin NiTi strips occurs in a sharp band, $55^o$ to the
                  loading axis. The transformation spreads by steady-state
                  propagation of inclined bands similar to those in the
                  literature of Luders bands in fine grained steel strips. In
                  NiTi the propagation also occasionally spreads by a
                  criss-cross pattern of finger-like features. These features
                  are captured in finite element simulations of thin strips by
                  treating the material as a finitely deforming elasto-plastic
                  solid having trilinear up-down-up nominal stress-strain
                  response. The success of this simple continuum model strongly
                  suggests that continuum level effects are the dominant
                  players in the unstable behavior of such fine grained
                  materials.},
  keywords     = {Material Instabilities Nickel-Titanium NiTi strips Nucleation
                  Luders bands},
  hardcopy     = {yes},
  ecopy	       = {no}
}

@Article{shaw:2000,
