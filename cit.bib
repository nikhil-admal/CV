@Article{baskes:2000,
  author       = {M. I. Baskes},
  title	       = {Atomistic model of plutonium},
  journal      = phys-rev-b,
  year	       = 2000,
  volume       = 62,
  number       = 23,
  pages	       = {15532--15537},
  month	       = {dec},
  abstract     = {A modified embedded atom model of Pu has been developed. This
                  simple atomic level model is able to reproduce the extreme
                  anomalous behavior of this complex element. The calculated
                  energetics and volume of the seven stable phases of Pu are
