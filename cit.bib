@Article{abeyaratne:chu:1996,
  author       = {Abeyaratne, R. and Chu, C. and James, R. D.},
  title	       = {Kinetics of materials with wiggly energies: Theory and
                  application to the evolution of twinning microstructures in a
                  {Cu-Al-Ni} shape memory alloy},
  journal      = phil-mag-a,
  year	       = 1996,
  volume       = 73,
  number       = 2,
  pages	       = {457--497},
  month	       = feb,
  abstract     = {We analyze the kinetics of the transition between two
                  variants of martensite during biaxial dead loading. The
                  volume fraction of one martensite variant versus the applied
                  loads exhibits an unusual hysteresis, characterized by a
                  sensitive dependence on the amplitude of the loads and a
                  dissipationless response at small amplitude. Observation of
                  the microscopic volume fraction at the level of a few bands
                  of martensite reveals that the main mechanism by which one
                  variant grows at the expense of another is a tip-splitting
                  event; the tips of martensite needles present in the specimen
                  suddenly split. This leads us to adopt a form of the energy
                  in which many little wiggles are superposed on a slowly
                  varying function that accounts for the loading device,
                  elastic and interfacial energies. We analyze the resulting
                  microscopic kinetic law by deriving from it a macroscopic
                  kinetic equation that governs the average response. This law
                  inherits the phenomenon of `getting stuck in local minima of
                  the energy'. It leads to good qualitative (and fair
                  quantitative) agreement with observation over a very wide
                  range of different kinds of experiment.},
  keywords     = {wiggly-energies twinning-microstructures
                  Cu-Al-Ni-shape-memory-alloy martensitic-transformations
                  biaxial-dead-loading volume-fraction dissipationless-response
                  tip-splitting-event interfacial-energies elastic-energies
                  Cu-Al-Ni},
  hardcopy     = {yes},
  ecopy	       = {no}
}

@Article{abeyaratne:knowles:1993,
  author       = {Rohan Abeyaratne and James K. Knowles},
  title	       = {A Continuum Model of a Thermoelastic Solid Capable of
                  Undergoing Phase Transitions},
  journal      = j-mech-phys,
  year	       = 1993,
  volume       = 41,
  number       = 3,
  pages	       = {541--571},
  month	       = mar,
  abstract     = {Constructs explicitly a Helmholtz free energy, a kinetic
                  relation and a nucleation criterion for a one-dimensional
                  thermoelastic solid, capable of undergoing either
                  mechanically- or thermally-induced phase transitions. The
                  authors study the hysteretic macroscopic response predicted
                  by this model in the case of quasistatic processes involving
                  stress cycling at constant temperature, thermal cycling at
                  constant stress, or a combination of mechanical and thermal
                  loading that gives rise to the shape-memory effect. These
                  predictions are compared qualitatively with experimental
                  results},
  keywords     = {free energy martensitic transformations shape memory effects
                  solid-state phase transformations thermoelasticity},
  hardcopy     = {yes},
  ecopy	       = {no}
}

@Article{abeyaratne:vedantam:2003,
  author       = {Rohan Abeyaratne and Srikanth Vedantam},
  title	       = {A lattice-based model of the kinetics of twin boundary
                  motion},
  journal      = j-mech-phys,
  year	       = 2003,
  volume       = 51,
  number       = 9,
  pages	       = {1675--1700},
  month	       = sep,
  abstract     = {In this paper we derive a macroscopic kinetic law for twin
                  boundary motion from a lattice dynamical model. The model is
                  developed for compound and type-1 twins and it is explicitly
                  illustrated for a Cu-Al-Ni shape memory alloy. The governing
                  multiple-well energy is calculated using an effective
                  interatomic potential; a Frenkel-Kontorowa type model is
                  developed for the dynamics at the lattice scale; and a
                  quasi-continuum approximation is used to determine the
                  continuum-scale kinetics. The model predicts that compound
                  twins in the Cu-Al-Ni shape memory alloy are an order of
                  magnitude more mobile than type-1 twins which is consistent
                  with experimental observations.},
  keywords     = {Shape memory effect Copper Lattice vibrations Crystal
                  lattices Crystallography Phase transitions Twinning
                  Mathematical Models},
  hardcopy     = {yes},
  ecopy	       = {yes}
}

